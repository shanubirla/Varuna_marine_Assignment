{
  "name": "fuel-eu-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/infrastructure/server/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/infrastructure/server/index.js",
    "migrate": "node ./scripts/run-sql.mjs src/infrastructure/db/migrations/001_init.sql",
    "seed": "tsx src/infrastructure/db/seed.ts",
    "reseed": "npm run migrate && npm run seed",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.4.5",
    "vitest": "^2.0.5"
  }
}
